<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.electapps.customcameratest">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.electapps.customcameratest.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.electapps.customcameratest.activitytest.CameraActivity"/>
        <activity android:name="com.electapps.customcameratest.PictureActivity"/>
        <activity android:name="com.electapps.customcameratest.fragmenttest.FragmentCameraActivity"/>
    </application>

</manifest>
